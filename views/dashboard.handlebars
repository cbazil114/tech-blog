<form class = "dashboardForm">
    <input placeholder="Title" id="postTitle">
    <input placeholder="Content" id="postContent">
    <button type="submit">Submit</button>
</form>
{{#each posts as |post| }}
<div>
    <h2>
        {{ post.title }}
    </h2>
    <p>
        {{ post.content }}
    </p>
    <p>
    {{ post.date_created }}
    </p>
    
</div>
{{/each}}
<script src="/js/post.js"></script>